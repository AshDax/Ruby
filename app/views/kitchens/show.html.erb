<h1>Kitchen Items</h1>

<ul>
  <% @kitchen.items.each do |item| %>
    
    <li>
      <%= item.name %>
    </li>     
    
  <% end %>

 <li> <%= link_to "Edit", edit_kitchen_path(@kitchen) %> </li>

 <li> <%= link_to "Delete", kitchen_path(@kitchen), method: :delete, data: {
                    "turbo_method": :delete
                  } %> </li>

</ul>